ta({ ...formData, [e.target.name]: e.target.value })\r\n\r\n  const onSubmit = async e => {\r\n    e.preventDefault();\r\n    const id = uuidv4();\r\n    if(password !== password2) {\r\n      dispatch(setAlert('Passwords do not match', 'danger', id));\r\n      setTimeout(() => {dispatch(removeAlert(id))}, 5000)\r\n    } else {\r\n      console.log('SUCCESS')\r\n    }\r\n  }\r\n\r\n  return (\r\n    <Fragment>\r\n      <h1 className=\"large text-primary\">Sign Up</h1>\r\n      <p className=\"lead\"><i className=\"fas fa-user\"></i> Create Your Account</p>\r\n      <form className=\"form\" onSubmit={e => onSubmit(e)}>\r\n        <div className=\"form-group\">\r\n          <input type=\"text\" placeholder=\"Name\" name=\"name\" value={name} onChange={e => onChange(e)} required />\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <input type=\"email\" placeholder=\"Email Address\" name=\"email\" value={email} onChange={e => onChange(e)} required />\r\n          <small className=\"form-text\"\r\n          >This site uses Gravatar so if you want a profile image, use a\r\n            Gravatar email</small>\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <input\r\n            type=\"password\"\r\n            placeholder=\"Password\"\r\n            name=\"password\"\r\n            value={password} \r\n            onChange={e => onChange(e)}\r\n            minLength=\"6\"\r\n          />\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <input\r\n            type=\"password\"\r\n            placeholder=\"Confirm Password\"\r\n            name=\"password2\"\r\n            value={password2} \r\n            onChange={e => onChange(e)}\r\n            minLength=\"6\"\r\n          />\r\n        </div>\r\n        <input type=\"submit\" className=\"btn btn-primary\" value=\"Register\" />\r\n      </form>\r\n      <p className=\"my-1\">\r\n        Already have an account? <Link to=\"/login\">Sign In</Link>\r\n      </p>\r\n    </Fragment>\r\n  )\r\n}\r\n\r\n// Register.propTypes = {\r\n//   setAlert: PropTypes.func.isRequired\r\n// }\r\n\r\n// export default connect(null, { setAlert })(Register);\r\nexport default Register;\r\n","C:\\Users\\Igor\\Desktop\\programming_folders\\udemy\\devconnector-2020\\client\\src\\components\\auth\\Login.js",[],"C:\\Users\\Igor\\Desktop\\programming_folders\\udemy\\devconnector-2020\\client\\src\\store.js",[],["64","65"],"C:\\Users\\Igor\\Desktop\\programming_folders\\udemy\\devconnector-2020\\client\\src\\reducers\\index.js",[],"C:\\Users\\Igor\\Desktop\\programming_folders\\udemy\\devconnector-2020\\client\\src\\reducers\\alert.js",["66"],"import { SET_ALERT, REMOVE_ALERT } from '../actions/types'\r\n\r\nconst initialState = [];\r\n\r\nexport default function(state = initialState, action) {\r\n  const { type, payload } = action;\r\n\r\n  switch(type) {\r\n    case SET_ALERT:\r\n      console.log('IN REDUCER - SET ALERT: ', state, payload)\r\n      return [...state, payload];\r\n\r\n    case REMOVE_ALERT:\r\n      console.log('PAYLOAD IN REMOVE ALERT: ', payload)\r\n      const filtered = state.filter(alert => alert.id !== payload)\r\n      console.log('IN REDUCER - REMOVE ALERT: ', filtered)\r\n      return state.filter(alert => alert.id !== payload);\r\n\r\n    default:\r\n      return state;\r\n  }\r\n}","C:\\Users\\Igor\\Desktop\\programming_folders\\udemy\\devconnector-2020\\client\\src\\actions\\types.js",[],"C:\\Users\\Igor\\Desktop\\programming_folders\\udemy\\devconnector-2020\\client\\src\\actions\\alert.js",[],"C:\\Users\\Igor\\Desktop\\programming_folders\\udemy\\devconnector-2020\\client\\src\\context.js",[],"C:\\Users\\Igor\\Desktop\\programming_folders\\udemy\\devconnector-2020\\client\\src\\components\\layout\\Alert.js",["67"],"import React, { Fragment, useContext } from 'react';\r\n// import PropTypes from 'prop-types';\r\n// import { connect } from 'react-redux';\r\nimport RootContext from '../../context';\r\n\r\nconst Aler